{
	"name": "webpack-sample",
	"displayName": "webpack-sample",
	"description": "",
	"version": "0.0.1",
	"publisher": "vscode-samples",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"engines": {
		"vscode": "^1.100.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./dist/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWebpack",
				"title": "Hello Webpack"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"test-compile": "tsc -p ./",
		"lint": "eslint",
		"start": "npx ts-node src/app.ts",
		"dev": "npx nodemon --exec ts-node src/app.ts",
		"migrate": "ts-node src/database/migration.ts",
		"build": "tsc",
		"test": "node ./test/runTest.js"
	},
	"nodemonConfig": {
		"watch": [
			"src"
		],
		"ext": ".ts,.js",
		"ignore": [],
		"exec": "ts-node ./src/app.ts",
		"start": "node dist/app.js",
		"dev": "nodemon",
		"build": "tsc"
	},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@stylistic/eslint-plugin": "^2.9.0",
		"@types/body-parser": "^1.19.6",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.3",
		"@types/node": "^22",
		"@types/vscode": "^1.103.0",
		"eslint": "^9.13.0",
		"ts-loader": "^7.0.5",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.39.0",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1"
	},
	"dependencies": {
		"body-parser": "^2.2.0",
		"cors": "^2.8.5",
		"dotenv": "^17.2.1",
		"ejs": "^3.1.10",
		"express": "^5.1.0",
		"mysql2": "^3.14.3"
	}
}
